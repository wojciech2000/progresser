{
  "name": "progresser",
  "version": "1.0.0",
  "description": "Web app to measure physical progress",
  "main": "index.js",
  "engines": {
    "node": "10.16.3",
    "npm": "6.14.5"
  },
  "scripts": {
    "start": "node server",
    "client": "cd client && npm run start",
    "sass": "node-sass client/src/App.scss client/src/App.css -w",
    "dev": "concurrently \"npm run start\" \"npm run client\" \" npm run sass \"",
    "heroku-postbuild": "NPM_CONFIG_PRODUCTION=false cd client && npm install && npm run build"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.3",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^5.9.16",
    "multer": "^1.4.2",
    "node-sass": "^4.14.1"
  },
  "devDependencies": {
    "concurrently": "^5.2.0"
  }
}
